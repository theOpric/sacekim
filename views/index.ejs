<script>
let IndexLang = navigator.language || navigator.userLanguage;
if (!Cookies.get('lang')) {
    if (IndexLang == 'tr') {
        Cookies.set('lang', 'TR');
        currentLang = 'TR';
    }
    else if (IndexLang == 'en') {
        Cookies.set('lang', 'EN');
        currentLang = 'EN';
    }
    else if (IndexLang == 'ru') {
        Cookies.set('lang', 'RU');
        currentLang = 'RU';
    }
    else if (IndexLang == 'fr') {
        Cookies.set('lang', 'FR');
        currentLang = 'FR';
    }
    else if (IndexLang == 'ar') {
        Cookies.set('lang', 'AR');
        currentLang = 'AR';
    }
    else if (IndexLang == 'pt') {
        Cookies.set('lang', 'PT');
        currentLang = 'PT';
    }
    else if (IndexLang == 'it') {
        Cookies.set('lang', 'IT');
        currentLang = 'IT';
    }
    else if (IndexLang == 'de') {
        Cookies.set('lang', 'DE');
        currentLang = 'DE';
    }
    else if (IndexLang == 'es') {
        Cookies.set('lang', 'ES');
        currentLang = 'ES';
    }
    else {
        Cookies.set('lang', 'TR');
        currentLang = 'TR';
    };
}
    let homeSlider = <%-JSON.stringify(slider)%>;
    let beforeAfter = <%-JSON.stringify(beforeAfter)%>;
    let lang = Cookies.get('lang');
    for (let i = 0; i < homeSlider.length; i++) {
        $("#homeSlider").append(`
        <div class="slider-item" style="background-image: url(`+homeSlider[i].gorsel+`);">
            <div class="overlay"></div>
            <div class="container">
                <div class="row no-gutters slider-text align-items-center justify-content-start" data-scrollax-parent="true">
                    <div class="col-md-6 text animate">
                        <h1 class="mb-4"> `+ (lang == 'TR' ? homeSlider[i].baslikKisim1TR : (lang == 'EN' ? homeSlider[i].baslikKisim1EN : (lang == 'ES' ? homeSlider[i].baslikKisim1ES : (lang == 'FR' ? homeSlider[i].baslikKisim1FR : (lang == 'DE' ? homeSlider[i].baslikKisim1DE : (lang == 'IT' ? homeSlider[i].baslikKisim1IT : (lang == 'AR' ? homeSlider[i].baslikKisim1AR : (lang == 'PT' ? homeSlider[i].baslikKisim1PT : homeSlider[i].baslikKisim1RU)))))))) + ` <span>` + (lang == 'TR' ? homeSlider[i].baslikKisim2TR : (lang == 'EN' ? homeSlider[i].baslikKisim2EN : (lang == 'ES' ? homeSlider[i].baslikKisim2ES : (lang == 'FR' ? homeSlider[i].baslikKisim2FR : (lang == 'DE' ? homeSlider[i].baslikKisim2DE : (lang == 'IT' ? homeSlider[i].baslikKisim2IT : (lang == 'AR' ? homeSlider[i].baslikKisim2AR : (lang == 'PT' ? homeSlider[i].baslikKisim2PT : homeSlider[i].baslikKisim2RU)))))))) + `</span></h1>
                        <h3 class="subheading">
                            `+ (lang == 'TR' ? homeSlider[i].baslikKisim3TR : (lang == 'EN' ? homeSlider[i].baslikKisim3EN : (lang == 'ES' ? homeSlider[i].baslikKisim3ES : (lang == 'FR' ? homeSlider[i].baslikKisim3FR : (lang == 'DE' ? homeSlider[i].baslikKisim3DE : (lang == 'IT' ? homeSlider[i].baslikKisim3IT : (lang == 'AR' ? homeSlider[i].baslikKisim3AR : (lang == 'PT' ? homeSlider[i].baslikKisim3PT : homeSlider[i].baslikKisim3RU)))))))) + `
                        </h3>
                    </div>
                </div>
            </div>
        </div>
        
        `);
    };
        for (let i = 0; i < beforeAfter.length; i++) {
        $("#beforeAfter").append(`
        <div class="item">
            <div class="testimony-wrap d-flex justify-content-center">
                <div class="ba-user-img mr-4" style="background-image: url(`+beforeAfter[i].before+`)">
                </div>
                <div class="ba-user-img mr-4"style="background-image: url(`+beforeAfter[i].after+`)">
                </div>
            </div>
        </div>
        `);
        };
</script>
<script>
    let blogs = <%-JSON.stringify(blog)%>;
    for (let i = 0; i < (blogs.length > 2 ? 3 : blogs.length); i++) {
        let quill = new Quill("#quills", {
            theme: 'snow',
            disable:true
        });
        quill.setContents((lang == 'TR' ? blogs[i].yaziTR : (lang == 'EN' ? blogs[i].yaziEN : (lang == 'ES' ? blogs[i].yaziES : (lang == 'FR' ? blogs[i].yaziFR : (lang == 'DE' ? blogs[i].yaziDE : (lang == 'IT' ? blogs[i].yaziIT : (lang == 'AR' ? blogs[i].yaziAR : (lang == 'PT' ? blogs[i].yaziPT : blogs[i].yaziRU)))))))));
        
        $("#blogs").append(`
        <div class="col-md-4">
                <div class="blog-entry">
                    <a id="`+blogs[i]._id+`" href="/blogicerigi" class="block-20 howBlogText"
                        style="background-image: url('`+blogs[i].gorsel+`');">
                        <div class="meta-date text-center p-2">
                            <span class="day">`+moment().format("DD/MM/YYYY")+`</span>
                        </div>
                    </a>
                    <div class="text bg-white p-4">
                        <h3 class="heading"><a href="/blogicerigi" id="`+blogs[i]._id+`" class="howBlogText"><span>` + (lang == 'TR' ? blogs[i].baslikTR : (lang == 'EN' ? blogs[i].baslikEN : (lang == 'ES' ? blogs[i].baslikES : (lang == 'FR' ? blogs[i].baslikFR : (lang == 'DE' ? blogs[i].baslikDE : (lang == 'IT' ? blogs[i].baslikIT : (lang == 'AR' ? blogs[i].baslikAR : (lang == 'PT' ? blogs[i].baslikPT :blogs[i].baslikRU)))))))) + `</a></h3>
                        <p>`+quill.getText(0,100)+`...</p>
                        <div class="d-flex align-items-center mt-4">
                            <p class="mb-0"><a href="/blogicerigi" id="`+blogs[i]._id+`" class="btn btn-primary howBlogText"><span style="font-size:36px"
                                        class="ion-ios-arrow-round-forward"></span></a></p>
                        </div>
                    </div>
                </div>
        </div>
        `);
        $(".ql-toolbar").remove();
    };
    $(".howBlogText").click(function () {
        let blogID = $(this).attr("id");
        Cookies.set("blogID",blogID);
    });
</script>
